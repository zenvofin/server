{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:best-practices"
  ],
  "nuget": {
    "enabled": true
  },
  "dockerfile": {
    "enabled": true
  },
  "github-actions": {
    "enabled": true
  },
  "schedule": [
    "at any time"
  ],
  "prHourlyLimit": 3,
  "prConcurrentLimit": 10
}
